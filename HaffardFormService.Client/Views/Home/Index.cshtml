@{
    ViewData["Title"] = "Dynamic Form";
}

<h2>Select Industry</h2>
<select id="industryType" class="form-control">
    <option value="">-- Select Industry --</option>
    @foreach (var industry in Model)
    {
        <option value="@industry">@industry</option>
    }
</select>

<button id="loadFields" class="btn btn-primary mt-3">Load Fields</button>

<div id="formContainer" class="mt-4"></div>

@section Scripts {
    <script src="/js/site.js"></script>
}


